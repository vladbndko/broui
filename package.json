{
  "name": "@vladbndko/brovue",
  "version": "0.0.5",
  "description": "Vue 3 components",
  "author": "Vladyslav Bondarenko <vladbndko@gmail.com>",
  "license": "MIT",
  "repository": "vladbndko/brovue",
  "type": "module",
  "keywords": [
    "components",
    "ui",
    "vue3",
    "vue",
    "vuejs"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/brovue.cjs",
  "module": "./dist/brovue.js",
  "exports": {
    ".": {
      "import": "./dist/brovue.js",
      "require": "./dist/brovue.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tabler/icons-vue": "^3.5.0",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "defu": "^6.1.4",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "prettier": "3.3.0",
    "sass": "^1.77.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.19"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "theme-colors": "^0.1.0"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "sideEffects": [
    "**/*.css"
  ]
}
